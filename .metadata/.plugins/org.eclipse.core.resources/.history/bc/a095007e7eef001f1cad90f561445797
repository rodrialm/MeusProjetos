package proativa.projeto.visao;

import javafx.application.Application;
import javafx.fxml.FXMLLoader;
import javafx.scene.Parent;
import javafx.scene.Scene;
import javafx.stage.Stage;

public class Cenas extends Application{

	Stage cena;
	
	String arquivosCSS = getClass().getResource("Layout.css").toExternalForm();
	Parent parent;
	int largura;
	int altura; 
	
	@Override
	public void start(Stage primaryStage) throws Exception {
		// TODO Auto-generated method stub
		
	}
	
	public Cenas() {
		criarCenaPrincipal();
	}
	
	
	
	
	public Cenas(Parent parent, int largura, int altura) {
		super();
		this.parent = parent;
		this.largura = largura;
		this.altura = altura;
	}

	public void criarCenaPrincipal() {
		
	}
	
	public void criarCenaCarregamento() {
		this.cena = new Scene(parent, largura, altura);
		
	}
}
