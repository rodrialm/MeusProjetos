package proativa.projeto.modelo;

import java.io.File;

import org.apache.poi.hssf.usermodel.HSSFWorkbook;

import javafx.fxml.FXML;
import javafx.stage.FileChooser;

public class Controlador {
	
	@FXML
	private FileChooser encontrarArquivo = new FileChooser();
	@FXML
	HSSFWorkbook arquivoEscolhido = new HSSFWorkbook();
	public void buscarArquivo() {
		encontrarArquivo.getExtensionFilters().add(new FileChooser
				.ExtensionFilter("Arquivos XML", "*.xml"));
		System.out.println("Pudin");
	}
}
